<core:FragmentDefinition xmlns:m="sap.m" xmlns:core="sap.ui.core" xmlns:fb="sap.ui.comp.filterbar" xmlns="sap.ui.comp.valuehelpdialog">
	
		<ValueHelpDialog title="Material" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" afterClose=".onValueHelpAfterClose"
		
		descriptionKey="ProductDescription" key="Material" supportMultiselect="false" >
			<filterBar>
				<fb:FilterBar advancedMode="false" search=".onFilterBarSearch">
					<fb:filterGroupItems>
						<fb:FilterGroupItem groupName="__$INTERNAL$" name="A" label="Plant" visibleInFilterBar="true">
							<fb:control>
								<m:Input id="PlantId" placeholder="Enter Plant" required="true"/>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="__$INTERNAL$" name="B" label="BOM Usage" visibleInFilterBar="true">
							<fb:control>
								<m:Input id="BOMId" placeholder="Enter BOM Usage" required="true"/>
							</fb:control>
						</fb:FilterGroupItem>
					</fb:filterGroupItems>
				</fb:FilterBar>
			</filterBar>
		</ValueHelpDialog>
	
</core:FragmentDefinition>